---
layout: none
---

{% assign normal = site.categories.apk | where: "piracy", "false" %}

[
  {% for post in normal %}
    {
      "title"    : "{{ post.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "tags"     : "{{ post.tags | join: ', ' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]